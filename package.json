{
	"name": "remote-wsl-recommender",
	"displayName": "%displayName%",
	"description": "%description%",
	"publisher": "ms-vscode-remote",
	"icon": "resources/remote-wsl.png",
	"version": "0.0.20",
	"license": "MIT",
	"repository": {
		"type": "git",
		"url": "https://github.com/Microsoft/vscode-remote-wsl-recommender.git"
	},
	"bugs": {
		"url": "https://github.com/Microsoft/vscode-remote-release/issues"
	},
	"engines": {
		"vscode": "^1.94.0"
	},
	"categories": [
		"Other"
	],
	"keywords": [
		"remote",
		"wsl",
		"remote development"
	],
	"aiKey": "0c6ae279ed8443289764825290e4f9e2-1a736e7c-1324-4338-be46-fc2a58ae4d14-7255",
	"activationEvents": [
		"onStartupFinished"
	],
	"main": "./dist/extension.js",
	"capabilities": {
		"virtualWorkspaces": true,
		"untrustedWorkspaces": {
			"supported": true
		}
	},
	"contributes": {
		"commands": [
			{
				"command": "remote-wsl-recommender.connectToWSL",
				"title": "%connectToWSLCommand%",
				"category": "WSL"
			},
			{
				"command": "remote-wsl-recommender.gettingStarted",
				"title": "%gettingStartedCommand%",
				"category": "WSL"
			}
		],
		"menus": {
			"commandPalette": [
				{
					"command": "remote-wsl-recommender.connectToWSL",
					"when": "remote-wsl-recommender.connectToWSL.available"
				},
				{
					"command": "remote-wsl-recommender.gettingStarted",
					"when": "false"
				}
			],
			"statusBar/remoteIndicator": [
				{
					"command": "remote-wsl-recommender.connectToWSL",
					"when": "remote-wsl-recommender.connectToWSL.available",
					"group": "remote_10_wsl_0_local@2"
				},
				{
					"command": "remote-wsl-recommender.gettingStarted",
					"when": "remote-wsl-recommender.gettingStarted.available",
					"group": "remote_10_wsl_0_local@9"
				}
			]
		}
	},
	"scripts": {
		"vscode:prepublish": "webpack --mode production",
		"compile": "webpack --mode none && npm run lint",
		"watch": "webpack --mode none --watch",
		"lint": "eslint src --ext ts",
		"preversion": "npm run compile",
		"postversion": "git push && git push --tags",
		"patch-insiders": "npm run compile && node ./build/patch-local.js .vscode-insiders",
		"patch-dev": "npm run compile && node ./build/patch-local.js .vscode-oss-dev"
	},
	"devDependencies": {
		"@vscode/extension-telemetry": "0.9.7",
		"vscode-nls": "^5.2.0",
		"vscode-tas-client": "^0.1.84",
		"@types/copy-webpack-plugin": "^10.1.0",
		"@types/node": "18.x",
		"@types/vscode": "^1.94.0",
		"@typescript-eslint/eslint-plugin": "^8.10.0",
		"@typescript-eslint/parser": "^8.10.0",
		"copy-webpack-plugin": "^12.0.2",
		"eslint": "^8.57.1",
		"ts-loader": "^9.5.1",
		"typescript": "^5.6.3",
		"vscode-nls-dev": "^4.0.4",
		"webpack": "^5.95.0",
		"webpack-cli": "^5.0.1"
	}
}
